<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox spacing="10" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <Label text="Edit Name"/>
    <TextField fx:id="nameField"/>
    <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button defaultButton="true" onAction="#goToDescriptionPage" text="Next"/>
            <Button cancelButton="true" onAction="#handleCancel" text="Cancel"/>
        </buttons>
    </ButtonBar>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
